{
  "name": "Digital Voltmeter",
  "version": "2",
  "manifest_version": 2,
  "permissions": [ "serial"],
  "minimum_chrome_version": "60",
  "app": {
    "default_icon": "icon_48.png",
    "background": {
      "container": "tab",
       "scripts": [ "background.js" ]
    }
  },
  
  "icons": {
    "16": "assets/icon_16.png",
    "38": "assets/icon_48.png",
    "48": "assets/icon_48.png",
    "128": "assets/icon_128.png"
  }
}