{
  "name": "Digital Voltmeter",
  "version": "1",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": [
        "js/background.js"
      ]
    }
  },
  "icons": {
    "16": "assets/icon_16.png",
    "38": "assets/icon_48.png",
    "48": "assets/icon_48.png",
    "128": "assets/icon_128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "serial"
  ]
}