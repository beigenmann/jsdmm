{
  "name": "Digital Voltmeter",
  "version": "2",
  "manifest_version": 2,
  "permissions": [ "tabs", "activeTab" ,"serial"],
  "minimum_chrome_version": "60",
  "icons": {
    "16": "assets/icon_16.png",
    "38": "assets/icon_48.png",
    "48": "assets/icon_48.png",
    "128": "assets/icon_128.png"
  },
  "browser_action": {
    "default_title": "Digital Voltmeter",
    "default_popup": "index.html#/dmm"
  },
  "background": {
    "page": "index.html#/dmm",
    "persistent": false
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
  
  
}